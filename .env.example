# Sandkasten environment variables
# Copy this file to .env and customize

# API key for authentication (leave empty for dev mode with no auth)
SANDKASTEN_API_KEY=sk-sandbox-dev

# Session TTL in seconds (default: 1800 = 30 minutes)
SANDKASTEN_SESSION_TTL_SECONDS=1800

# Allowed images (comma-separated)
# SANDKASTEN_ALLOWED_IMAGES=sandbox-runtime:base,sandbox-runtime:python,sandbox-runtime:node

# Resource limits
# SANDKASTEN_CPU_LIMIT=1.0
# SANDKASTEN_MEM_LIMIT_MB=512
# SANDKASTEN_PIDS_LIMIT=256
# SANDKASTEN_MAX_EXEC_TIMEOUT_MS=120000

# Network mode: "none" or "full"
# SANDKASTEN_NETWORK_MODE=none
